{
    "name": "vue-mdi-icon",
    "version": "0.1.10",
    "description": "Provides Icon component for Vue that uses the https://materialdesignicons.com/ icons through @mdi/js",
    "author": "Earl Pomeroy <earl.pomeroy42@gmail.com>",
    "license": "BSD-3-Clause",
    "licenses": [
        {
            "name": "BSD-3-Clause",
            "url": "https://opensource.org/licenses/BSD-3-Clause"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/EarlPomeroy/vue-mdi-icon.git"
    },
    "bugs": {
        "url": "https://github.com/EarlPomeroy/vue-mdi-icon/issues"
    },
    "keywords": [
        "Vue",
        "mdi",
        "materialdesingicons",
        "icon",
        "component"
    ],
    "main": "dist/vue-mdi-icon.ssr.js",
    "browser": "dist/vue-mdi-icon.esm.js",
    "module": "dist/vue-mdi-icon.esm.js",
    "unpkg": "dist/vue-mdi-icon.min.js",
    "files": [
        "dist/*",
        "src/**/*.vue"
    ],
    "scripts": {
        "serve": "vue-cli-service serve dev/serve.js",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@mdi/js": "^7.4.47",
        "@rollup/plugin-alias": "^4.0.3",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@vue/cli-service": "^5.0.8",
        "cross-env": "^7.0.2",
        "minimist": "^1.2.5",
        "prettier": "^2.0.5",
        "rollup": "^2.79.1",
        "rollup-plugin-vue": "^6.0.0",
        "vue": "^2.7.14"
    },
    "peerDependencies": {
        "vue": "^2.6.11"
    },
    "engines": {
        "node": ">=10"
    }
}
